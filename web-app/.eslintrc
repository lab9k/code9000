{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "no-return-assign": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "arrow-body-style": 0,
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0,
    "import/no-named-as-default": 0,
    "jsx-a11y/label-has-for": 0,
    "arrow-parens": "off",
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0
  }
}